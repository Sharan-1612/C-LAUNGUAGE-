#include <stdio.h>
void main() {
    int a;
      printf(" Enter the year : ");
      scanf("%d",&a);
      if(a%400==0)
        printf("It is a leap year");
      else if (a%100==0)
          printf("It's a leap year");
      else if(a%4==0)
          printf(" It's is a leap year ");
         else
            printf("it's not a leap year ");
}
